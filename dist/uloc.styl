$toggle-width       = 40px
$toggle-height      = 21px
$toggle-handle-size = $toggle-height
$toggle-base-height = round($toggle-handle-size / 1.7)
.u-toggle-base
  transition all .45s cubic-bezier(.23, 1, .32, 1)
  width 100%
  height $toggle-base-height
  border-radius 30px
  background currentColor
  opacity .5
.u-toggle-handle
  background rgb(245, 245, 245)
  transition all 450ms cubic-bezier(.23, 1, .32, 1)
  box-shadow $shadow-1
  border-radius 50%
  position absolute
  top 0
  left 0
  width $toggle-handle-size
  height $toggle-handle-size
  line-height $toggle-handle-size
.u-toggle .u-option-inner
  height $toggle-height
  width $toggle-width
  min-width $toggle-width
  padding round(($toggle-height - $toggle-base-height) / 2) 0
  &.active
    .u-toggle-handle
      background currentColor
      left ($toggle-width - $toggle-handle-size)
    .u-toggle-icon
      color white
.u-toolbar
  padding $toolbar-padding
  min-height $toolbar-min-height
  overflow hidden
  width 100%
.u-toolbar-inverted
  background white
.u-toolbar-title
  flex 1
  min-width 1px
  max-width 100%
  font-size $toolbar-title-font-size
  font-weight $toolbar-title-font-weight
  padding $toolbar-title-padding
.u-toolbar-subtitle
  font-size $toolbar-subtitle-font-size
  opacity .7
.u-toolbar-title, .u-toolbar-subtitle
  text-overflow ellipsis
  white-space nowrap
  overflow hidden
.u-tooltip
  position fixed
  color $tooltip-color
  background $tooltip-background
  box-shadow $tooltip-box-shadow
  z-index $z-tooltip
  padding $tooltip-padding
  border-radius $tooltip-border-radius
  overflow-y auto
  overflow-x hidden
  pointer-events none
.u-tree-node
  margin 0
  list-style-type none
  position relative
  padding 0 0 3px 22px
  &:after
    content ''
    position absolute
    top -3px
    bottom 0
    width 1px
    right auto
    left -13px
    border-left 1px solid currentColor
  &:last-child:after
    display none
.u-tree-node-header:before
  content ''
  position absolute
  top -3px
  bottom 50%
  width 35px
  left -35px
  border-left 1px solid currentColor
  border-bottom 1px solid currentColor
.u-tree-children
  padding-left 25px
  &.disabled
    pointer-events none
.u-tree-node-body
  padding 5px 0 8px 5px
.u-tree-node-parent
  padding-left 2px
  > .u-tree-node-header:before
    width 15px
    left -15px
  > .u-tree-node-collapsible > .u-tree-node-body
    padding 5px 0 8px 27px
    &:after
      content ''
      position absolute
      top 0
      width 1px
      height 100%
      right auto
      left 12px
      border-left 1px solid currentColor
      bottom 50px
.u-tree-node-link
  cursor pointer
.u-tree-node-selected
  background rgba(0, 0, 0, .15)
.u-tree-dark .u-tree-node-selected
  background rgba(255, 255, 255, .4)
body.desktop
  .u-tree-node-link:hover
    background rgba(0, 0, 0, .1)
  .u-tree-dark .u-tree-node-link:hover
    background rgba(255, 255, 255, .3)
.u-tree-node-header
  padding 4px
  margin-top 3px
  border-radius $generic-border-radius
  &.disabled
    pointer-events none
.u-tree-icon
  font-size 1.5em
.u-tree-img
  height 3em
  &.avatar
    width 2em
    height 2em
.u-tree-arrow
  font-size 1rem
  width 1rem
  height 1rem
.u-tree-arrow-rotate
  transform rotate(90deg)
[dir=rtl]
  .u-tree-arrow
    transform rotate(180deg) /* rtl:ignore */
  .u-tree-arrow-rotate
    transform rotate(90deg) /* rtl:ignore */
.u-tree
  
  > .u-tree-node
    padding 0
    &:after, > .u-tree-node-header:before
      display none
  
  > .u-tree-node-child > .u-tree-node-header
    padding-left 24px
.u-uploader-expanded .u-if
  border-bottom-left-radius 0
  border-bottom-right-radius 0
.u-uploader-input
  opacity 0
  max-width 100%
  height 100%
  width 100%
  font-size 0
.u-uploader-pick-button[disabled] .u-uploader-input
  display none
.u-uploader-files
  border 1px solid $grey-4
  font-size 14px
  max-height 500px
.u-uploader-files-no-border .u-uploader-files
  border-top 0 !important
.u-uploader-file:not(:last-child)
  border-bottom 1px solid $grey-4
.u-uploader-progress-bg, .u-uploader-progress-text
  pointer-events none
.u-uploader-progress-bg
  height 100%
  opacity .2
.u-uploader-progress-text
  font-size 40px
  opacity .1
  right 44px
  bottom 0
.u-uploader-dnd
  outline 2px dashed currentColor
  outline-offset -6px
  background rgba(255, 255, 255, .6)
  &.inverted
