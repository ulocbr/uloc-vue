    width: 100%;
    float: left;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
  }
  .loading {
    width: 100%;
    display: inline-block;
    position: relative;
    margin: 0;
    
    background-repeat: no-repeat;
    background-size: 100% 100%;
    background-position: center;
    animation-name: rotate;
    animation-duration: 550ms;
